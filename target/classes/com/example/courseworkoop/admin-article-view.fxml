<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@articleViewCSS.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.courseworkoop.AdminArticleViewController">
    <children>
        <Label fx:id="usernameLabel" layoutX="538.0" layoutY="21.0" prefHeight="19.0" prefWidth="185.0" styleClass="welcome-name-label" text="" />
        <Label fx:id="titleLabel" layoutX="36.0" layoutY="74.0" maxWidth="550.0" styleClass="article-title-label" text="Title" wrapText="true" />
        <Label fx:id="descriptionLabel" layoutX="36.0" layoutY="208.0" maxWidth="550.0" styleClass="article-description-label" text="Description" wrapText="true" />
        <Label fx:id="urlLabel" layoutX="34.0" layoutY="164.0" styleClass="article-url-label" text="URL" />
        <Button fx:id="backButton" layoutX="24.0" layoutY="21.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
        <Button fx:id="deleteButton" layoutX="324.0" layoutY="332.0" mnemonicParsing="false" onAction="#onDeleteArticleButtonClick" text="Delete Article" />
    </children>
</AnchorPane>
