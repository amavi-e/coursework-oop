        <Label fx:id="titleLabel" layoutX="36.0" layoutY="74.0" maxWidth="550.0" styleClass="article-title-label" text="Title" wrapText="true" />
        <Label fx:id="descriptionLabel" layoutX="34.0" layoutY="192.0" maxWidth="550.0" styleClass="article-description-label" text="Description" wrapText="true" />
