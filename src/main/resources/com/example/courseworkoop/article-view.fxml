<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@articleViewCSS.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.courseworkoop.ArticleViewController">
    <children>
        <Label fx:id="titleLabel" layoutX="25.0" layoutY="74.0" maxWidth="550.0" styleClass="article-title-label" text="Title" wrapText="true" />
        <Label layoutX="25.0" layoutY="110.0" text="" />
        <Label fx:id="descriptionLabel" layoutX="25.0" layoutY="171.0" maxWidth="550.0" styleClass="article-description-label" text="Description" wrapText="true" />
        <Label layoutX="25.0" layoutY="170.0" text="" />
        <Label fx:id="contentLabel" layoutX="28.0" layoutY="264.0" maxWidth="550.0" prefWidth="560.0" styleClass="article-content-label" text="Content" wrapText="true" />
        <Button fx:id="backButton" layoutX="24.0" layoutY="21.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
        <Label fx:id="usernameLabel" layoutX="500.0" layoutY="26.0" styleClass="welcome-name-label" text="" />
        <Button fx:id="likeButton" layoutX="201.0" layoutY="23.0" mnemonicParsing="false" onAction="#onLikeButtonClick" text="Like article" />
        <Button fx:id="dislikeButton" layoutX="297.0" layoutY="23.0" mnemonicParsing="false" onAction="#onDislikeButtonClick" text="Dislike article" />

    </children>
</AnchorPane>
